<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.bytefinger.toutuo.common.mapper.CommonMapper">    <select id="listCommonName" resultType="com.bytefinger.common.core.web.domain.vo.CommonNameVO">        SELECT        id,        ${columnName}_name as `name`        FROM        biz_${tableName}        WHERE        id in        <foreach collection="ids" index="index" item="item" open="(" separator="," close=")">            #{item}        </foreach>    </select>    <select id="listBizData" resultType="java.util.Map">        ${sql}    </select>    <update id="updateBiz">        ${sql}    </update>    <select id="listBizUser" resultType="com.bytefinger.toutuo.common.domain.BizUserBaseEntity">        SELECT        b.*        FROM        biz_${tableName} a        INNER JOIN biz_${tableName}_team b        ON(a.id = b.record_id)        WHERE        b.record_id in        <foreach collection="ids" index="index" item="item" open="(" separator="," close=")">            #{item}        </foreach>    </select></mapper>